package example

object listsSheet {

	val xs = List(1, 2, 3)                    //> xs  : List[Int] = List(1, 2, 3)
	val ans = 0
  def sum(xs: List[Int]): Int = 1                 //> sum: (xs: List[Int])Int
      if (xs.isEmpty) ans else println("nenen")
                                                  //> nenen
}